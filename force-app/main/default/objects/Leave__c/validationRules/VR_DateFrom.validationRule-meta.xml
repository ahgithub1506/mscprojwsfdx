<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>VR_DateFrom</fullName>
    <active>true</active>
    <description>Date cannot be in the past.</description>
    <errorConditionFormula>Date_From__c &lt; TODAY()</errorConditionFormula>
    <errorDisplayField>Date_From__c</errorDisplayField>
    <errorMessage>Date cannot be in the past.</errorMessage>
</ValidationRule>
